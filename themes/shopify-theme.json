    "list.activeSelectionBackground": "#133A49",
    "list.inactiveSelectionBackground": "#0F1B2E",
    "sideBar.foreground": "#95A7D5",
    "tree.indentGuidesStroke": "#95A7D5",
    "list.activeSelectionForeground": "#FFF",
    "list.hoverForeground": "#FFF",
    "list.focusForeground": "#FFF",
    "quickInput.background": "#0F1B2E",
    "quickInput.foreground": "#95A7D5",
    "quickInputTitle.background": "#0F1B2E",
    "quickInputList.focusBackground": "#133A49",
    "quickInputList.focusForeground": "#FFF",
    "icon.foreground": "#95A7D5",
    "symbolIcon.colorForeground": "#95A7D5",
    "scrollbar.shadow": "#0F1B2E",
    "scrollbarSlider.background": "#2A3A52",
    "scrollbarSlider.hoverBackground": "#364669",
    "scrollbarSlider.activeBackground": "#9DB8F7",
    "editor.findMatchBackground": "#5870A4",
    "editor.findMatchBorder": "#9DB8F7",
    "editor.findMatchHighlightBackground": "#133A49",
    "editor.findMatchHighlightBorder": "#5870A4",
    "editor.findRangeHighlightBackground": "#1A273F",
    "searchEditor.findMatchBackground": "#5870A4",
    "searchEditor.findMatchBorder": "#9DB8F7",
    "searchEditor.textInputBorder": "#5870A4"
  },
  "tokenColors": [
    {
      "name": "JSON Object Keys",
      "scope": [
        "support.type.property-name.json",
        "meta.structure.dictionary.key.json",
        "string.quoted.double.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#A4DFFD"
      }
    },
    {
      "name": "HTML Tags",
      "scope": [
        "entity.name.tag.html",
        "entity.name.tag.jsx",
        "entity.name.tag.tsx"
      ],
      "settings": {
        "foreground": "#DDE2F1"
      }
    },
    {
      "name": "HTML Brackets",
      "scope": [
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.definition.tag.jsx",
        "punctuation.definition.tag.tsx"
      ],
      "settings": {
        "foreground": "#EFF1F1"
      }
    },
    {
      "name": "Text Content",
      "scope": [
        "text.html",
        "source.js",
        "source.jsx",
        "source.tsx",
        "source.css",
        "text.xml"
      ],
      "settings": {
        "foreground": "#A4DFFD"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.jsx",
        "entity.other.attribute-name.tsx",
        "entity.other.attribute-name.class.html",
        "entity.other.attribute-name.id.html"
      ],
      "settings": {
        "foreground": "#9DB8F7"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string.quoted.double",
        "string.quoted.single",
        "string.template",
        "string.interpolated"
      ],
      "settings": {
        "foreground": "#FE9F9B"
      }
    },
    {
      "name": "Liquid Tags",
      "scope": [
        "punctuation.definition.liquid",
        "entity.name.tag.liquid",
        "meta.tag.liquid"
      ],
      "settings": {
        "foreground": "#A4DFFD"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword.control",
        "keyword.control.conditional",
        "keyword.control.loop",
        "keyword.liquid"
      ],
      "settings": {
        "foreground": "#9DB8F7"
      }
    },
    {
      "name": "Liquid Functions",
      "scope": ["support.function.liquid", "entity.name.function.liquid"],
      "settings": {
        "foreground": "#F3F972"
      }
    },
    {
      "name": "Liquid Objects",
      "scope": [
        "variable.other.liquid",
        "punctuation.definition.variable.liquid"
      ],
      "settings": {
        "foreground": "#08F4B9"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric.liquid", "constant.numeric"],
      "settings": {
        "foreground": "#62F44C"
      }
    },
    {
      "name": "Variables and Functions",
      "scope": ["variable.other", "entity.name.function"],
      "settings": {
        "foreground": "#A4DFFD"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#95A7D5",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Liquid Comments",
      "scope": [
        "comment.block.liquid",
        "punctuation.definition.comment.liquid"
      ],
      "settings": {
        "foreground": "#D7D7DB",
        "fontStyle": "italic"
      }
    }
  ]
}
