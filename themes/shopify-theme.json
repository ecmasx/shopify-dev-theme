    "tab.unfocusedHoverBackground": "#0F1B2E",
    "tab.unfocusedHoverBorder": "#00000000",
    "tab.border": "#00000000",
    "breadcrumb.background": "#364669",
    "breadcrumb.foreground": "#475B86",
    "breadcrumb.focusForeground": "#95A7D5",
    "breadcrumb.activeSelectionForeground": "#95A7D5",
    "editorGroupHeader.tabsBackground": "#364669",
    "editorGroupHeader.tabsBorder": "#1A273F",
