    "editor.findMatchBackground": "#8F5DA5",
    "editor.findMatchBorder": "#D793BB",
    "editor.findMatchHighlightBackground": "#2F093C",
    "editor.findMatchHighlightBorder": "#8F5DA5",
    "editor.findRangeHighlightBackground": "#25152D",
    "searchEditor.findMatchBackground": "#8F5DA5",
    "searchEditor.findMatchBorder": "#D793BB",
    "searchEditor.textInputBorder": "#8F5DA5",
    "notification.background": "#462B52",
    "notification.foreground": "#D793BB",
    "notification.border": "#8F5DA5",
    "notification.buttonBackground": "#25152D",
    "notification.buttonForeground": "#D793BB",
    "notification.buttonBorder": "#8F5DA5",
    "notification.infoBackground": "#462B52",
    "notification.infoForeground": "#A4DFFD",
    "notification.warningBackground": "#462B52",
    "notification.warningForeground": "#BE7782",
    "notification.errorBackground": "#462B52",
    "notification.errorForeground": "#BE7782"
  },
  "tokenColors": [
    {
      "name": "JSON Object Keys",
      "scope": [
        "support.type.property-name.json",
        "meta.structure.dictionary.key.json",
        "string.quoted.double.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#A4DFFD"
      }
    },
    {
      "name": "HTML Tags",
      "scope": [
        "entity.name.tag.html",
        "entity.name.tag.jsx",
        "entity.name.tag.tsx"
      ],
      "settings": {
        "foreground": "#FEE8F4"
      }
    },
    {
      "name": "HTML Brackets & Punctuation",
      "scope": [
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.definition.tag.jsx",
        "punctuation.definition.tag.tsx",
        "punctuation.definition.block",
        "punctuation.definition.parameters",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#D793BB"
      }
    },
    {
      "name": "Text Content",
      "scope": [
        "text.html",
        "source.js",
        "source.jsx",
        "source.tsx",
        "source.css",
        "text.xml"
      ],
      "settings": {
        "foreground": "#FEE8F4"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.jsx",
        "entity.other.attribute-name.tsx",
        "entity.other.attribute-name.class.html",
        "entity.other.attribute-name.id.html"
      ],
      "settings": {
        "foreground": "#D793BB"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string.quoted.double",
        "string.quoted.single",
        "string.template",
        "string.interpolated"
      ],
      "settings": {
        "foreground": "#BE7782"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable.other",
        "variable.parameter",
        "variable.language",
        "variable.other.liquid",
        "punctuation.definition.variable.liquid"
      ],
      "settings": {
        "foreground": "#08F4B9"
      }
    },
    {
      "name": "Functions & Keywords",
      "scope": [
        "keyword.control",
        "keyword.control.conditional",
        "keyword.control.loop",
        "keyword.liquid",
        "keyword.function",
        "storage.type.function",
        "keyword.other",
        "storage.type"
      ],
      "settings": {
        "foreground": "#8F5DA5"
      }
    },
    {
      "name": "Function Names & Calls",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "entity.name.function.js",
        "entity.name.function.liquid"
      ],
      "settings": {
        "foreground": "#FEE8F4"
      }
    },
    {
      "name": "Liquid Tags",
      "scope": [
        "punctuation.definition.liquid",
        "entity.name.tag.liquid",
        "meta.tag.liquid"
      ],
      "settings": {
        "foreground": "#FEE8F4"
      }
    },
    {
      "name": "Liquid Functions",
      "scope": ["support.function.liquid", "entity.name.function.liquid"],
      "settings": {
        "foreground": "#FEE8F4"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric.liquid", "constant.numeric"],
      "settings": {
        "foreground": "#62F44C"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "comment.line.double-slash",
        "comment.block"
      ],
      "settings": {
        "foreground": "#D7D7DB",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Liquid Comments",
      "scope": [
        "comment.block.liquid",
        "punctuation.definition.comment.liquid"
      ],
      "settings": {
        "foreground": "#D7D7DB",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Classes & Types",
      "scope": ["entity.name.class", "entity.name.type", "support.type"],
      "settings": {
        "foreground": "#8F5DA5"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.language",
        "constant.character",
        "variable.language.this"
      ],
      "settings": {
        "foreground": "#8F5DA5"
      }
    },
    {
      "name": "Parentheses and Brackets",
      "scope": [
        "punctuation.definition.parameters.begin",
        "punctuation.definition.parameters.end",
        "punctuation.definition.block.js",
        "punctuation.definition.block.jsx",
        "punctuation.definition.block.tsx",
        "meta.brace.round",
        "meta.brace.square",
        "meta.brace.curly"
      ],
      "settings": {
        "foreground": "#D793BB"
      }
    }
  ]
}
