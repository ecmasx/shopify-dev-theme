    "quickInput.background": "#151D1E",
    "quickInput.foreground": "#8A8F93",
    "quickInputTitle.background": "#151D1E",
    "quickInputList.focusBackground": "#20292B",
    "quickInputList.focusForeground": "#D7D7DB",
    "panel.border": "#00000000",
    "sideBar.border": "#00000000",
    "editorGroup.border": "#00000000",
    "activityBar.border": "#00000000",
    "statusBar.border": "#00000000",
    "sideBar.foreground": "#8A8F93",
    "tree.indentGuidesStroke": "#8A8F93",
    "list.activeSelectionForeground": "#FFF",
    "list.hoverForeground": "#FFF",
    "list.focusForeground": "#FFF",
    "icon.foreground": "#8A8F93",
    "symbolIcon.colorForeground": "#8A8F93",
    "scrollbar.shadow": "#151D1E",
    "scrollbarSlider.background": "#20292B",
    "scrollbarSlider.hoverBackground": "#0A1D26",
    "scrollbarSlider.activeBackground": "#4A9B8E",
    "editor.findMatchBackground": "#4A9B8E",
    "editor.findMatchBorder": "#59767A",
    "editor.findMatchHighlightBackground": "#0A1D26",
    "editor.findMatchHighlightBorder": "#4A9B8E",
    "editor.findRangeHighlightBackground": "#20292B",
    "searchEditor.findMatchBackground": "#4A9B8E",
    "searchEditor.findMatchBorder": "#59767A",
    "searchEditor.textInputBorder": "#59767A",
    "notification.background": "#304B4F",
    "notification.foreground": "#8A8F93",
    "notification.border": "#59767A",
    "notification.buttonBackground": "#20292B",
    "notification.buttonForeground": "#8A8F93",
    "notification.buttonBorder": "#59767A",
    "notification.infoBackground": "#304B4F",
    "notification.infoForeground": "#A4DFFD",
    "notification.warningBackground": "#304B4F",
    "notification.warningForeground": "#FE9F9B",
    "notification.errorBackground": "#304B4F",
    "notification.errorForeground": "#FE9F9B",
    "editorWidget.background": "#20292B",
    "editorWidget.foreground": "#8A8F93",
    "editorWidget.resizeBorder": "#59767A",
    "pickerGroup.border": "#59767A",
    "pickerGroup.foreground": "#4A9B8E",
    "quickInputTitle.background": "#20292B"
  },
  "tokenColors": [
    {
      "name": "JSON Object Keys",
      "scope": [
        "support.type.property-name.json",
        "meta.structure.dictionary.key.json",
        "string.quoted.double.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#4A9B8E"
      }
    },
    {
      "name": "HTML Tags",
      "scope": [
        "entity.name.tag.html",
        "entity.name.tag.jsx",
        "entity.name.tag.tsx"
      ],
      "settings": {
        "foreground": "#EFF1F1"
      }
    },
    {
      "name": "HTML Brackets & Punctuation",
      "scope": [
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.definition.tag.jsx",
        "punctuation.definition.tag.tsx",
        "punctuation.definition.block",
        "punctuation.definition.parameters",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#EFF1F1"
      }
    },
    {
      "name": "Text Content",
      "scope": [
        "text.html",
        "source.js",
        "source.jsx",
        "source.tsx",
        "source.css",
        "text.xml"
      ],
      "settings": {
        "foreground": "#8A8F93"
      }
    },
    {
      "name": "HTML Classes",
      "scope": [
        "entity.other.attribute-name.class.html",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.class"
      ],
      "settings": {
        "foreground": "#FFD602"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.jsx",
        "entity.other.attribute-name.tsx",
        "entity.other.attribute-name.class.html",
        "entity.other.attribute-name.id.html"
      ],
      "settings": {
        "foreground": "#6BB6A8"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string.quoted.double",
        "string.quoted.single",
        "string.template",
        "string.interpolated"
      ],
      "settings": {
        "foreground": "#FE9F9B"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable.other",
        "variable.parameter",
        "variable.language",
        "variable.other.liquid",
        "punctuation.definition.variable.liquid"
      ],
      "settings": {
        "foreground": "#08F4B9"
      }
    },
    {
      "name": "Functions & Keywords",
      "scope": [
        "keyword.control",
        "keyword.control.conditional",
        "keyword.control.loop",
        "keyword.liquid",
        "keyword.function",
        "storage.type.function",
        "keyword.other",
        "storage.type"
      ],
      "settings": {
        "foreground": "#9DB8F7"
      }
    },
    {
      "name": "Function Names & Calls",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "entity.name.function.js",
        "entity.name.function.liquid"
      ],
      "settings": {
        "foreground": "#9DB8F7"
      }
    },
    {
      "name": "Function Parameters",
      "scope": [
        "variable.parameter.function",
        "meta.function.parameters variable",
        "meta.function-call.arguments variable"
      ],
      "settings": {
        "foreground": "#08F4B9"
      }
    },
    {
      "name": "Liquid Tags",
      "scope": [
        "punctuation.definition.liquid",
        "entity.name.tag.liquid",
        "meta.tag.liquid"
      ],
      "settings": {
        "foreground": "#A4DFFD"
      }
    },
    {
      "name": "Liquid Objects",
      "scope": ["support.variable.liquid", "variable.other.liquid"],
      "settings": {
        "foreground": "#08F4B9"
      }
    },
    {
      "name": "Liquid Conditions",
      "scope": [
        "keyword.control.liquid",
        "keyword.control.conditional.liquid",
        "keyword.control.loop.liquid"
      ],
      "settings": {
        "foreground": "#9DB8F7"
      }
    },
    {
      "name": "Settings Properties",
      "scope": [
        "variable.other.property",
        "support.variable.property",
        "meta.property-name"
      ],
      "settings": {
        "foreground": "#A2DDFA"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric.liquid", "constant.numeric"],
      "settings": {
        "foreground": "#79A7FF"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "comment.line.double-slash",
        "comment.block"
      ],
      "settings": {
        "foreground": "#D7D7DB",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Liquid Comments",
      "scope": [
        "comment.block.liquid",
        "punctuation.definition.comment.liquid"
      ],
      "settings": {
        "foreground": "#D7D7DB",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Classes & Types",
      "scope": ["entity.name.class", "entity.name.type", "support.type"],
      "settings": {
        "foreground": "#6BB6A8"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.language",
        "constant.character",
        "variable.language.this"
      ],
      "settings": {
        "foreground": "#4A9B8E"
      }
    },
    {
      "name": "CSS Property Names",
      "scope": [
        "support.type.property-name.css",
        "support.type.vendored.property-name.css",
        "variable.other.property.css",
        "variable.css",
        "variable.scss",
        "variable.other.less"
      ],
      "settings": {
        "foreground": "#EFF1F1"
      }
    },
    {
      "name": "Parentheses and Brackets",
      "scope": [
        "punctuation.definition.parameters.begin",
        "punctuation.definition.parameters.end",
        "punctuation.definition.block.js",
        "punctuation.definition.block.jsx",
        "punctuation.definition.block.tsx",
        "meta.brace.round",
        "meta.brace.square",
        "meta.brace.curly"
      ],
      "settings": {
        "foreground": "#EFF1F1"
      }
    }
  ]
}
