  "tokenColors": [
    {
      "name": "JSON Object Keys",
      "scope": [
        "support.type.property-name.json",
        "meta.structure.dictionary.key.json",
        "string.quoted.double.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#A4DFFD"
      }
    },
    {
      "name": "HTML Tags",
      "scope": [
        "entity.name.tag.html",
        "entity.name.tag.jsx",
        "entity.name.tag.tsx"
      ],
      "settings": {
        "foreground": "#D8F6D6"
      }
    },
    {
      "name": "HTML Brackets & Punctuation",
      "scope": [
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.definition.tag.jsx",
        "punctuation.definition.tag.tsx",
        "punctuation.definition.block",
        "punctuation.definition.parameters",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#A4DFFD"
      }
    },
    {
      "name": "Text Content",
      "scope": [
        "text.html",
        "source.js",
        "source.jsx",
        "source.tsx",
        "source.css",
        "text.xml"
      ],
      "settings": {
        "foreground": "#A4DFFD"
      }
    },
    {
      "name": "HTML Classes",
      "scope": [
        "entity.other.attribute-name.class.html",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.class"
      ],
      "settings": {
        "foreground": "#F3F972"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.jsx",
        "entity.other.attribute-name.tsx",
        "entity.other.attribute-name.class.html",
        "entity.other.attribute-name.id.html"
      ],
      "settings": {
        "foreground": "#9DB8F7"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string.quoted.double",
        "string.quoted.single",
        "string.template",
        "string.interpolated"
      ],
      "settings": {
        "foreground": "#FE9F9B"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable.other",
        "variable.parameter",
        "variable.language",
        "variable.other.liquid",
        "punctuation.definition.variable.liquid"
      ],
      "settings": {
        "foreground": "#08F4B9"
      }
    },
    {
      "name": "Functions & Keywords",
      "scope": [
        "keyword.control",
        "keyword.control.conditional",
        "keyword.control.loop",
        "keyword.liquid",
        "keyword.function",
        "storage.type.function",
        "keyword.other",
        "storage.type"
      ],
      "settings": {
        "foreground": "#9DB8F7"
      }
    },
    {
      "name": "Function Names & Calls",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "entity.name.function.js",
        "entity.name.function.liquid"
      ],
      "settings": {
        "foreground": "#A4DFFD"
      }
    },
    {
      "name": "Function Parameters",
      "scope": [
        "variable.parameter.function",
        "meta.function.parameters variable",
        "meta.function-call.arguments variable"
      ],
      "settings": {
        "foreground": "#08F4B9"
      }
    },
    {
      "name": "Liquid Tags",
      "scope": [
        "punctuation.definition.liquid",
        "entity.name.tag.liquid",
        "meta.tag.liquid"
      ],
      "settings": {
        "foreground": "#A4DFFD"
      }
    },
    {
      "name": "Liquid Functions",
      "scope": ["support.function.liquid", "entity.name.function.liquid"],
      "settings": {
        "foreground": "#F3F972"
      }
    },
    {
      "name": "Liquid Objects",
      "scope": [
        "variable.other.liquid",
        "punctuation.definition.variable.liquid",
        "support.variable.liquid"
      ],
      "settings": {
        "foreground": "#08F4B9"
      }
    },
    {
      "name": "Liquid Conditions",
      "scope": [
        "keyword.control.liquid",
        "keyword.control.conditional.liquid",
        "keyword.control.loop.liquid"
      ],
      "settings": {
        "foreground": "#9DB8F7"
      }
    },
    {
      "name": "Settings Properties",
      "scope": [
        "variable.other.property",
        "support.variable.property",
        "meta.property-name"
      ],
      "settings": {
        "foreground": "#A4DFFD"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric.liquid", "constant.numeric"],
      "settings": {
        "foreground": "#62F44C"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "comment.line.double-slash",
        "comment.block"
      ],
      "settings": {
        "foreground": "#95A7D5",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Liquid Comments",
      "scope": [
        "comment.block.liquid",
        "punctuation.definition.comment.liquid"
      ],
      "settings": {
        "foreground": "#D7D7DB",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Classes & Types",
      "scope": ["entity.name.class", "entity.name.type", "support.type"],
      "settings": {
        "foreground": "#9DB8F7"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.language",
        "constant.character",
        "variable.language.this"
      ],
      "settings": {
        "foreground": "#9DB8F7"
      }
    },
    {
      "name": "CSS Property Names",
      "scope": [
        "support.type.property-name.css",
        "support.type.vendored.property-name.css",
        "variable.other.property.css",
        "variable.css",
        "variable.scss",
        "variable.other.less"
      ],
      "settings": {
        "foreground": "#DDE2F1"
      }
    },
    {
      "name": "Parentheses and Brackets",
      "scope": [
        "punctuation.definition.parameters.begin",
        "punctuation.definition.parameters.end",
        "punctuation.definition.block.js",
        "punctuation.definition.block.jsx",
        "punctuation.definition.block.tsx",
        "meta.brace.round",
        "meta.brace.square",
        "meta.brace.curly"
      ],
      "settings": {
        "foreground": "#EFF1F1"
      }
    }
  ]
}
