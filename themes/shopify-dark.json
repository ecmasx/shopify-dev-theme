{
  "name": "Shopify Theme Dev - Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#364669",
    "sideBar.background": "#0F1B2E",
    "activityBar.background": "#0F1B2E",
    "panel.background": "#0F1B2E",
    "terminal.background": "#0F1B2E",
    "statusBar.background": "#0F1B2E",
    "titleBar.activeBackground": "#0F1B2E",
    "menu.background": "#0F1B2E",
    "dropdown.background": "#0F1B2E",
    "input.background": "#0F1B2E",
    "sideBar.foreground": "#95A7D5",
    "tree.indentGuidesStroke": "#95A7D5",
    "list.activeSelectionForeground": "#FFF",
    "list.hoverForeground": "#FFF",
    "list.focusForeground": "#FFF",
    "icon.foreground": "#95A7D5",
    "symbolIcon.colorForeground": "#95A7D5"
  },
  "tokenColors": [
    {
      "name": "HTML Tags",
      "scope": [
        "entity.name.tag.html",
        "entity.name.tag.jsx",
        "entity.name.tag.tsx"
      ],
      "settings": {
        "foreground": "#D8F6D6"
      }
    },
    {
      "name": "HTML Brackets",
      "scope": [
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.definition.tag.jsx",
        "punctuation.definition.tag.tsx"
      ],
      "settings": {
        "foreground": "#EFF1F1"
      }
    },
    {
      "name": "Text Content",
      "scope": [
        "text.html",
        "source.js",
        "source.jsx",
        "source.tsx",
        "source.css",
        "text.xml"
      ],
      "settings": {
        "foreground": "#A4DFFD"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.jsx",
        "entity.other.attribute-name.tsx",
        "entity.other.attribute-name.class.html",
        "entity.other.attribute-name.id.html"
      ],
      "settings": {
        "foreground": "#9DB8F7"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string.quoted.double",
        "string.quoted.single",
        "string.template",
        "string.interpolated"
      ],
      "settings": {
        "foreground": "#FE9F9B"
      }
    },
    {
      "name": "Liquid Tags",
      "scope": [
        "punctuation.definition.liquid",
        "entity.name.tag.liquid",
        "meta.tag.liquid"
      ],
      "settings": {
        "foreground": "#A4DFFD"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword.control",
        "keyword.control.conditional",
        "keyword.control.loop",
        "keyword.liquid"
      ],
      "settings": {
        "foreground": "#9DB8F7"
      }
    },
    {
      "name": "Liquid Functions",
      "scope": ["support.function.liquid", "entity.name.function.liquid"],
      "settings": {
        "foreground": "#F3F972"
      }
    },
    {
      "name": "Liquid Objects",
      "scope": [
        "variable.other.liquid",
        "punctuation.definition.variable.liquid"
      ],
      "settings": {
        "foreground": "#08F4B9"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric.liquid", "constant.numeric"],
      "settings": {
        "foreground": "#62F44C"
      }
    },
    {
      "name": "Variables and Functions",
      "scope": ["variable.other", "entity.name.function"],
      "settings": {
        "foreground": "#A4DFFD"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#95A7D5",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Liquid Comments",
      "scope": [
        "comment.block.liquid",
        "punctuation.definition.comment.liquid"
      ],
      "settings": {
        "foreground": "#D7D7DB",
        "fontStyle": "italic"
      }
    }
  ]
}
